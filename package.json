{
  "name": "automationexercicedotcom",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "cypress:verify": "cypress verify",
    "test:user": "cypress run --spec \"cypress/e2e/User/**/*.cy.js\"",
    "test:shop": "cypress run --spec \"cypress/e2e/Shop/**/*.cy.js\"",
    "test:checkout": "cypress run --spec \"cypress/e2e/Checkout/**/*.cy.js\"",
    "test:contact": "cypress run --spec \"cypress/e2e/Contact/**/*.cy.js\"",
    "test:ui": "cypress run --spec \"cypress/e2e/UI_misc/**/*.cy.js\"",
    "test:smoke": "cypress run --spec \"cypress/e2e/User/login.cy.js,cypress/e2e/Shop/shop.cy.js\"",
    "test:regression": "cypress run",
    "report:open": "open cypress/reports/html/index.html",
    "clean:reports": "rm -rf cypress/reports",
    "clean:screenshots": "rm -rf cypress/screenshots",
    "clean:all": "npm run clean:reports && npm run clean:screenshots"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "cypress": "^14.5.4",
    "cypress-mochawesome-reporter": "^4.0.2",
    "mocha": "^11.7.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
